        <ns0:plugins>
            <ns0:plugin>
                <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                <ns0:artifactId>maven-compiler-plugin</ns0:artifactId>
                <ns0:version>3.8.1</ns0:version>
                <ns0:configuration>
                    <ns0:source>17</ns0:source>
                    <ns0:target>17</ns0:target>
                </ns0:configuration>
            </ns0:plugin>
        </ns0:plugins>
    </ns0:build>

</ns0:project>